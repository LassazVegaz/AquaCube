<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#92DEF4"
    android:backgroundTint="#92DEF4"
    android:backgroundTintMode="src_in"
    android:foregroundTint="#92DEF4"
    android:foregroundTintMode="add"
    tools:context=".activities.hiruni.PopulationMenu">

    <TextView
        android:id="@+id/textView"
        android:layout_width="349dp"
        android:layout_height="86dp"
        android:text="Population Menu"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="32sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.935"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <Button
        android:id="@+id/button"
        android:layout_width="294dp"
        android:layout_height="75dp"
        android:backgroundTint="#92DEF4"
        android:text="Manage Population Data"
        android:textColor="#000000"
        android:textSize="16sp"
        app:cornerRadius="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.153"
        app:strokeColor="#03989E"
        app:strokeWidth="3sp" />

    <Button
        android:onClick="btnSocieties"
        android:id="@+id/button2"
        android:layout_width="289dp"
        android:layout_height="67dp"
        android:backgroundTint="#91DDF3"
        android:text="Manage Societies"
        android:textColor="#000000"
        android:textSize="16sp"
        app:cornerRadius="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        app:layout_constraintVertical_bias="0.03"
        app:strokeColor="#03989E"
        app:strokeWidth="3sp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="328dp"
        android:layout_height="281dp"
        android:layout_marginStart="42dp"
        android:layout_marginEnd="42dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintVertical_bias="0.76"
        app:srcCompat="@drawable/popmenu" />
</androidx.constraintlayout.widget.ConstraintLayout>