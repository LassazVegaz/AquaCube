<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.icecube">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IceCube"
        tools:targetApi="31">
        <activity
            android:name=".Home"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".CustomListView"
            android:exported="true">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ListView"
            android:exported="true">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".adddetails"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />
        <activity
            android:name=".MainActivity2"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />
        <activity
            android:name=".MainActivity3"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />
        <activity
            android:name=".MainActivity5"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />
        <activity
            android:name=".MainActivity6"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />
        <activity
            android:name=".AnotherActivity"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />

        <activity
            android:name=".register"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />
        <activity
            android:name=".RVActivity"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />
        <activity
            android:name=".CampaignList"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".CampaignOrg"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".campaigndetail"
            android:exported="true"
            android:label="Compaign details"
            android:parentActivityName=".campaignintroduction" />
        <activity
            android:name=".campaignintroduction"
            android:exported="true"
            android:label="AquaCube"
            android:parentActivityName=".CampaignList">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.IceCube.NoActionBar" />
    </application>

</manifest>