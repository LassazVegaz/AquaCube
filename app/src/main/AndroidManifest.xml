<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.icecube">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IceCube"
        tools:targetApi="31">
        <activity
            android:name=".SingleSociety"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>


        <activity
            android:name=".SinglePeople"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".SingleComplaint"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".RelavantPartiesActivity"
            android:exported="false"
            android:label="Unique Relavant parties"
            android:parentActivityName=".RelavantPartiesMain" />
        <activity
            android:name=".RelavantPartiesMain"
            android:exported="false"
            android:label="Relavant parties"
            android:parentActivityName=".ViewComplaintsActivity" />
        <activity
            android:name=".ViewComplaintsActivity"
            android:exported="false"
            android:label="Solve Complaints"
            android:parentActivityName=".ViewComplaintsMain" />
        <activity
            android:name=".ViewComplaintsMain"
            android:exported="false"
            android:label="View Complaints"
            android:parentActivityName=".Complaints" />
        <activity
            android:name=".EmailActivity"
            android:exported="false"
            android:label="View Complaints"
            android:parentActivityName=".Complaints" />
        <activity
            android:name=".PeopleList"
            android:exported="false"
            android:label="View Complaints"
            android:parentActivityName=".Complaints" />

        <activity
            android:name=".ManageSocieties"
            android:exported="false"
            android:label="View Complaints"
            android:parentActivityName=".Complaints" />

        <activity
            android:name=".PopulationMenu"
            android:exported="true"
            android:label="Inquries/Complaints"
            android:parentActivityName=".ComplaintsInsert">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.IceCube.NoActionBar" />
    </application>

</manifest>